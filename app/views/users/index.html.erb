<div class="page-header">
  <h3>My Wall</h3>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>User</th>
      </tr>

      <% @users.each do |user| %>
      <tr>
        <td>
          <a href="/users/<%= user.id %>"><%= user.username %></a></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
